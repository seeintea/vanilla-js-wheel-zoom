        "js",
        "javascript",
        "vanilla",
        "mousewheel",
        "image zoom",
        "image scale",
        "drag scrollable image",
        "zoom",
        "pinch-to-zoom"
    ],
    "author": "worka",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/seeintea/vanilla-js-wheel-zoom"
